{
    "name": "msgphp/eav-bundle",
    "type": "symfony-bundle",
    "description": "Basic entity-attribute-value management (the `EAV` domain)",
    "keywords": [
        "entity-attribute-value",
        "eav"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "require": {
        "msgphp/eav": "^0.10",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4 || ^4.0",
        "symfony/http-kernel": "^3.4 || ^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\EavBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
