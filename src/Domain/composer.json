{
    "name": "msgphp/domain",
    "type": "library",
    "description": "A dependency-free package to help building a business domain layer",
    "keywords": [
        "domain-driven-design",
        "ddd",
        "domain model",
        "rich domain model",
        "entity",
        "repository",
        "orm",
        "event sourcing",
        "es",
        "command query responsibility segregation",
        "cqrs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.0"
    },
    "conflict": {
        "symfony/messenger": "<4.2"
    },
    "require-dev": {
        "api-platform/core": "^2.2",
        "doctrine/doctrine-bundle": "^1.8",
        "doctrine/orm": "^2.6",
        "elasticsearch/elasticsearch": "^6.0",
        "psr/container": "^1.0",
        "ramsey/uuid": "^3.7",
        "ramsey/uuid-doctrine": "^1.4",
        "symfony/config": "^3.4 || ^4.0",
        "symfony/dependency-injection": "^3.4.16 || ^4.0.16 || ^4.1.5",
        "symfony/http-kernel": "^3.4 || ^4.0",
        "symfony/messenger": "^4.2",
        "symfony/var-exporter": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.9-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "MsgPhp\\Domain\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev"
}
